{
  "name": "@nrk/expect-openapi",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrkno/expect-openapi.git"
  },
  "description": "Expect objects to match OpenAPI documents",
  "license": "MIT",
  "author": {
    "name": "NRK",
    "email": "opensource@nrk.no",
    "url": "https://nrkbeta.no/"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "tsc -p ./tsconfig.build.json",
    "clean": "rimraf ./dist",
    "lint": "run-p lint:*",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:format": "yarn --silent prettier --write",
    "lint-fix:tslint": "yarn --silent tslint --fix",
    "lint:format": "yarn --silent prettier -l",
    "lint:tslint": "yarn --silent tslint",
    "prepare": "run-s build",
    "test": "run-s lint test:*",
    "test:jest": "jest",
    "tslint": "tslint -p . 'src/**/*.{ts,tsx}'",
    "prettier": "prettier \"src/**/*.{js,jsx,ts,less,tsx,json}\""
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "better-ajv-errors": "^0.6.4",
    "expect": "^24.8.0",
    "jest-matcher-utils": "^24.8.0",
    "json-schema-ref-parser": "^7.0.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/prettier": "^1.16.4",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.7",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
